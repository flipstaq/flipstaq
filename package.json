{
  "name": "@flipstaq/root",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "services/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "concurrently --names \"WEB,AUTH,USER,PRODUCT,GATEWAY\" --prefix-colors \"blue,green,yellow,cyan,magenta\" \"npm run dev --workspace=@flipstaq/web\" \"npm run start:dev --workspace=@flipstaq/auth-service\" \"npm run start:dev --workspace=@flipstaq/user-service\" \"npm run start:dev --workspace=@flipstaq/product-service\" \"npm run start:dev --workspace=@flipstaq/api-gateway\"",
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "turbo run clean",
    "db:generate": "turbo run db:generate",
    "db:push": "turbo run db:push",
    "db:studio": "turbo run db:studio"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.10",
    "@nestjs/core": "^10.3.10",
    "@nestjs/platform-express": "^10.3.10",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@prisma/client": "^6.9.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "turbo": "^1.13.4",
    "concurrently": "^8.2.2",
    "@nestjs/cli": "^10.4.4",
    "@nestjs/schematics": "^10.1.4",
    "@nestjs/testing": "^10.3.10",
    "prisma": "^6.9.0",
    "typescript": "^5.4.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
